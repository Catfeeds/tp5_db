* { margin: 0; padding: 0;}
body { background: #f9f9f9; font-size: 14px;}

ul li { margin: 0; padding: 0; list-style-type: none;}   /*  清除列表前面的点  */
a { text-decoration: none;}                              /*  清除字体下滑线  */
img { padding: 0; margin: 0; border: none;}              /*  清除边框  */
.left {float: left;}
.right { float:right;}
.clear { clear: both}
.w1024 { width: 1024px; margin: 0 auto;}
.w1041 { width: 1041px; margin: 0 auto;}
.red { color: red;}
.about { padding-top: 20px; background: #fff; overflow: hidden; padding-bottom: 30px;}
.about-list {width: 285px; overflow: hidden; text-align: center;  float: left;}
.about-list p {
    width: 285px;
    height: 100px;
    line-height: 100px;
    font-size: 30px;
    color: #fff;
    background: #005fb7;

}
.about-list ul {
    overflow: hidden;
    background: #e8e8e8;
    padding-bottom: 30px;
    padding-top: 10px;
}
.about-list ul li{
    height: 50px;
    line-height: 50px;
    font-size: 18px;
    margin-bottom: 5px;
}
.about-list ul li a:hover{
    color: #fff;
    background: #00519c;
}
.about-list ul li a{
    color: #000;
    display:block;
}
.about-list ul li a.onRed {
    color: #fff;
    background: #00519c;
}


.about-content { width: 700px;
    overflow: hidden;
    float: right;
}
/*分页样式*/
.pagination{
    width: 100%;
    height:50px;
    text-align: center;
    font-size:14px;
}
.pagination li{
    display: inline-block;
    width:50px;
    height: 25px;
    line-height: 25px;

    border-radius: 5px;
    margin-left: 5px;
    margin-right: 5px;
    background:#5eb95e;
    color:#fff !important;

}
.pagination a{
    color:#fff;
}


/* 养殖种类 */
.recommend {
    height: 300px;
}
 .recommend-img {
     width: 300px;
     float: left;
     margin: 20px;
     position: relative;
     height: 220px;


}
.recommend-img .image-img {
   position: relative;
}
.recommend-img .image-img img {
    width: 300px;
    height: 220px;
}
.recommend-img .recommend-title {
    padding: 10px;
    background: rgba(65, 84, 100, 0.85);
    line-height:25px;
    height: 25px;
    position: absolute;
    left: 20px;
    right: 20px;
    bottom: 20px;
    visibility: visible;
    text-align: center;
    color: #fff;
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    -ms-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
    transition: all 0.3s ease-in-out;

}

.recommend-img .recommend-content {
    position: absolute;
    top: 0px;
    left: 0;
    opacity: 0;
    visibility: hidden;
    height: 100%;
    background: rgba(65, 84, 100, 0.85);
    font-size: 14px;
    padding: 30px 30px 40px;
    transform: translateY(-100%);
    -webkit-transition: all 0.5s ease-in-out;
    -moz-transition: all 0.5s ease-in-out;
    -ms-transition: all 0.5s ease-in-out;
    -o-transition: all 0.5s ease-in-out;
    transition: all 0.5s ease-in-out;
}






